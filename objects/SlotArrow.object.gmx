    y = (parent.y);
    
    if (clicked) {
        clicked = false;
        
        if (canSwapSlot(parent)) {
            var slot = parent.slot;
            var altSlot = slot.alternativeSlot;
            swapSlot(slot, altSlot);
            parent.canMoveLane  = false;
        } else {
            show_debug_message("Alternative slot is full: " + string(parent.slot.alternativeSlot.card));
